{
  "cli": {
    "version": ">= 7.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging"
    },
    "production": {
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
